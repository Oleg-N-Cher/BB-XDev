MODULE Math;

CONST
  Pi* = 3.1415926535; E* = 2.7182818285;

PROCEDURE Sin* (x: REAL64): REAL32; END Sin;
PROCEDURE Cos* (x: REAL64): REAL32; END Cos;
PROCEDURE ArcTan* (x: REAL64): REAL32; END ArcTan;
PROCEDURE Exp* (x: REAL64): REAL32; END Exp;
PROCEDURE Sqrt* (x: REAL64): REAL32; END Sqrt;
PROCEDURE Ln* (x: REAL64): REAL32; END Ln;
PROCEDURE Pow* (x, y: REAL64): REAL32; END Pow;
PROCEDURE Randomize*; END Randomize;
PROCEDURE RndRange* (min, max: INT16): INT16;
  BEGIN RETURN 0 END RndRange;

END Math.
