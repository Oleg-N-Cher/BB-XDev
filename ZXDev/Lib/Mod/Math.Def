MODULE Math;

CONST
  Pi* = 3.1415926535; E* = 2.7182818285;
  
PROCEDURE Sin* (x: SHORTREAL): SHORTREAL; END Sin;
PROCEDURE Cos* (x: SHORTREAL): SHORTREAL; END Cos;
PROCEDURE ArcTan* (x: SHORTREAL): SHORTREAL; END ArcTan;
PROCEDURE Exp* (x: SHORTREAL): SHORTREAL; END Exp;
PROCEDURE Sqrt* (x: SHORTREAL): SHORTREAL; END Sqrt;
PROCEDURE Ln* (x: SHORTREAL): SHORTREAL; END Ln;

PROCEDURE Randomize*; END Randomize;
PROCEDURE RndRange* (min, max: SHORTINT): SHORTINT;
  BEGIN RETURN 0 END RndRange;

END Math.
