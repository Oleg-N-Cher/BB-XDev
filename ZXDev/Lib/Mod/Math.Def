MODULE Math;

CONST
  Pi* = 3.1415926535; E* = 2.7182818285;

PROCEDURE Sin* (x: REAL): SHORTREAL; END Sin;
PROCEDURE Cos* (x: REAL): SHORTREAL; END Cos;
PROCEDURE ArcTan* (x: REAL): SHORTREAL; END ArcTan;
PROCEDURE Exp* (x: REAL): SHORTREAL; END Exp;
PROCEDURE Sqrt* (x: REAL): SHORTREAL; END Sqrt;
PROCEDURE Ln* (x: REAL): SHORTREAL; END Ln;
PROCEDURE Pow* (x, y: REAL): SHORTREAL; END Pow;
PROCEDURE Randomize*; END Randomize;
PROCEDURE RndRange* (min, max: SHORTINT): SHORTINT;
  BEGIN RETURN 0 END RndRange;

END Math.
