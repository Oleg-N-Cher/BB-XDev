MODULE Math;

CONST
  Pi* = 3.1415926535; E* = 2.7182818285;
  
PROCEDURE Sin* (x: REAL): REAL; END Sin;
PROCEDURE Cos* (x: REAL): REAL; END Cos;
PROCEDURE ArcTan* (x: REAL): REAL; END ArcTan;
PROCEDURE Exp* (x: REAL): REAL; END Exp;
PROCEDURE Sqrt* (x: REAL): REAL; END Sqrt;
PROCEDURE Ln* (x: REAL): REAL; END Ln;

PROCEDURE Randomize*; END Randomize;
PROCEDURE RndRange* (min, max: SHORTINT): SHORTINT;
  BEGIN RETURN 0 END RndRange;

END Math.
