MODULE Strings;

TYPE
  CHAR = SHORTCHAR;

PROCEDURE Length* (s: ARRAY OF CHAR): SHORTINT; BEGIN RETURN 0 END Length;
PROCEDURE IntToStr* (n: SHORTINT; VAR str: ARRAY OF CHAR): BOOLEAN;
  BEGIN RETURN TRUE END IntToStr;
PROCEDURE UIntToStr* (n: SHORTINT; VAR str: ARRAY OF CHAR): BOOLEAN;
  BEGIN RETURN TRUE END UIntToStr;
PROCEDURE StrToInt* (str: ARRAY OF CHAR; VAR result: SHORTINT): BOOLEAN;
  BEGIN END StrToInt;

END Strings.
