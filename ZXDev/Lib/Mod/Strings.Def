MODULE Strings;

TYPE
  CHAR = SHORTCHAR;

PROCEDURE Length* (s: ARRAY OF CHAR): SHORTINT; END Length;
PROCEDURE IntToStr* (n: SHORTINT; VAR str: ARRAY OF CHAR): BOOLEAN;
  END IntToStr;
PROCEDURE UIntToStr* (n: SHORTINT; VAR str: ARRAY OF CHAR): BOOLEAN;
  END UIntToStr;
PROCEDURE StrToInt* (str: ARRAY OF CHAR; VAR result: SHORTINT): BOOLEAN;
  END StrToInt;

END Strings.
